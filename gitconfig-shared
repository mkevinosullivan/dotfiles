[alias]
        ac = commit --all
        acm = commit --all --message
        cm = commit --message
        cgl = config --global --list
        cl = config --list --show-origin
        co = checkout
        cob = checkout -b
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        irb = rebase --interactive
        last = log -1 HEAD --stat
        lp = push --force-with-lease
[rerere]
        enabled = true
[pull]
        rebase = true
[fetch]
        prune = true
[init]
        defaultBranch = main
